div
  div.progress
    div.state(style='width:{{state}}')
  div.editor.editor-left
    div.filePath
      select(ng-model='filePathA', ng-options='path for path in filePaths', ng-change='switchFile()')
    div.diff(ng-click='switchDiff("fileA")', ng-class='{loding:fileADiffLoading}')
    div(ng-bind-html='fileA')
  div.editor
    div.filePath
      select(ng-model='filePathB', ng-options='path for path in filePaths', ng-change='switchFile()')
    div.diff(ng-click='switchDiff("fileB")', ng-class='{loding:fileBDiffLoading}')
    div(ng-bind-html='fileB')
  div.btnContainer
    div.btnContainerMargin
    a.prev(href='{{prev || "javascript: void 0;"}}', ng-class='{disabled: !prev}') <&nbsp;
  div.btnContainer.btnContainer-right
    div.btnContainerMargin
    a.next(href='{{next || "javascript: void 0;"}}', ng-class='{disabled: !next}') >
  div.msg(style='{{msgPos()}}', ng-show='state') {{cmtMsg}}