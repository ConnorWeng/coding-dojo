    "jade": "~1.8.2",
    "less-middleware": "1.0.x",
    "date-utils": "^1.2.16",
    "gitlab": "^1.2.0",
    "q": "^1.1.2",
    "pygments": "^0.2.0"
  },
  "devDependencies": {
    "mocha": "^2.1.0",
    "chai": "~1.9.1",
    "sinon": "^1.9.1",
    "protractor": "^1.6.1",
    "karma": "^0.12.31",
    "jasmine-core": "^2.1.3",
    "karma-chrome-launcher": "^0.1.7",
    "karma-jasmine": "^0.1.5"
